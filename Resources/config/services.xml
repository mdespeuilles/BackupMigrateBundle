<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="mdespeuilles_backup_migrate.backup_command" class="Mdespeuilles\BackupMigrateBundle\Command\BackupCommand">
            <tag name="console.command" />
        </service>
        <service id="mdespeuilles_backup_migrate.restore_command" class="Mdespeuilles\BackupMigrateBundle\Command\RestoreCommand">
            <tag name="console.command" />
        </service>
    </services>
</container>
